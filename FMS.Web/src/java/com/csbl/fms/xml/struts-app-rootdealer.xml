    <struts>
        
	<package extends="base-struts" name="rootdealer-struts" namespace="/">

            <action name="rootdealerhome" class="com.csbl.fms.action.AppMenuAction" method="checkRootDealer">
                <result name="success" type="tiles">/rootdealerhome.tiles</result>
                <result name="login" type="tiles">/loginform.tiles</result>
            </action>
            
            <action name="dealerList" class="com.csbl.fms.action.AppMenuAction" method="checkRootDealer">
